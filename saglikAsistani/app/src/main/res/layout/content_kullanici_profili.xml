<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.muslu.saglikasistani.Kullanici_profili"
    tools:showIn="@layout/activity_kullanici_profili"
    android:background="@drawable/background"
    >

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/camera"
        android:id="@+id/resimler"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="323dp"
        android:layout_marginStart="323dp"
        tools:ignore="ContentDescription,RtlHardcoded" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:ignore="RtlHardcoded">

        <FrameLayout
            android:layout_width="112dp"
            android:layout_height="157dp"
            android:layout_gravity="left|top"
            tools:ignore="UselessLeaf">

        </FrameLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="153dp"
            android:layout_gravity="center"
            android:baselineAligned="false"

            android:weightSum="1">

            <TextView
                android:layout_width="87dp"
                android:layout_height="wrap_content"
                android:text="ÖDÜLLERİNİZ"
                android:id="@+id/textView7"
                android:textColor="@color/Yellow"
                android:textStyle="bold"
                android:layout_gravity="top|center"
                tools:ignore="HardcodedText" />

        </LinearLayout>

        <FrameLayout
            android:layout_width="372dp"
            android:layout_height="175dp"
            android:layout_gravity="center_horizontal|bottom"
            >

            <FrameLayout
                android:layout_width="105dp"
                android:layout_height="63dp"
                android:layout_gravity="center|center"
                android:background="#76FF03">


                <ImageButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/sure_id"
                    android:layout_gravity="center_horizontal|top"
                    android:background="@drawable/chronomether"
                    tools:ignore="ContentDescription" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="En uzun süre"
                    android:id="@+id/textView67"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="@android:color/white"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="süre"
                    android:id="@+id/textView66"
                    android:layout_gravity="center"
                    android:textColor="@android:color/white"
                    android:layout_marginTop="5dp"
                    tools:ignore="HardcodedText" />

            </FrameLayout>

            <FrameLayout
                android:layout_width="105dp"
                android:layout_height="63dp"
                android:layout_gravity="center|right"
                android:background="#64DD17">

                <ImageButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/kalori_id"
                    android:layout_gravity="center_horizontal|top"
                    android:background="@drawable/kalori"
                    tools:ignore="ContentDescription" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="yakılanKalori"
                    android:id="@+id/textView68"
                    android:layout_gravity="center"
                    android:textColor="@android:color/white"
                    android:layout_marginTop="5dp"
                    tools:ignore="HardcodedText" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="En çok kalori"
                    android:id="@+id/textView69"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="@android:color/white"
                    tools:ignore="HardcodedText" />

            </FrameLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="KİŞİSEL REKORLAR"
                android:id="@+id/textView8"
                android:layout_gravity="center|top"
                android:textColor="@color/Chartreuse"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />

            <FrameLayout
                android:layout_width="105dp"
                android:layout_height="63dp"
                android:layout_gravity="left|center"
                android:background="@color/ForestGreen">

                <ImageButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/adim_id"
                    android:layout_gravity="center_horizontal|top"
                    android:background="@drawable/adim"
                    tools:ignore="ContentDescription" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="adımSayısı"
                    android:id="@+id/textView9"
                    android:layout_gravity="center"
                    android:textColor="@android:color/white"
                    android:layout_marginTop="5dp"
                    tools:ignore="HardcodedText" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="En çok adım"
                    android:id="@+id/textView10"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="@android:color/white"
                    tools:ignore="HardcodedText" />
            </FrameLayout>
        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="86dp"
            android:layout_gravity="center">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton3"
                android:layout_gravity="left|center_vertical"
                android:background="@drawable/odul_1"
                tools:ignore="ContentDescription" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton4"
                android:layout_gravity="center"
                android:background="@drawable/odul_2"
                android:layout_marginRight="20dp"
                tools:ignore="ContentDescription,RtlHardcoded" />


            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/gecisTarih_id"
                android:onClick="Tiklandi2"
                android:layout_gravity="right|top"
                android:background="@drawable/gecis"
                tools:ignore="ContentDescription" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:text="tarihler gelecek"
                android:id="@+id/textView11"
                android:layout_gravity="left|bottom"
                tools:ignore="HardcodedText" />

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton5"
                android:layout_gravity="right|center_vertical"
                android:background="@drawable/odul_3"
                android:layout_marginRight="30dp"
                tools:ignore="ContentDescription,RtlHardcoded" />

        </FrameLayout>

        <FrameLayout
            android:layout_width="196dp"
            android:layout_height="143dp">

            <ListView
                android:id="@+id/liste_id"
                android:layout_width="match_parent"
                android:layout_height="196dp" />

        </FrameLayout>

    </FrameLayout>

</RelativeLayout>
