<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.muslu.saglikasistani.ogeleri_yonet"
    tools:showIn="@layout/activity_ogeleri_yonet"
    android:background="@drawable/background"
    >

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/su_sec">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="242dp"
            android:layout_gravity="center_horizontal|top">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="102dp"
                android:layout_gravity="left|top">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal|top">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView2"
                        android:layout_gravity="left|center_vertical"
                        android:background="@drawable/oge_su" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Su"
                        android:id="@+id/Su"
                        android:layout_gravity="center"
                        android:textStyle="bold"
                        android:textColor="@color/Black" />

                    <Switch
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/sec1_id"
                        android:layout_gravity="right|center_vertical" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="31dp"
                    android:layout_gravity="left|center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Kafein"
                        android:id="@+id/textView17"
                        android:layout_gravity="center"
                        android:textColor="@color/Black"
                        android:textStyle="bold" />

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="match_parent"
                        android:id="@+id/imageView3"
                        android:layout_gravity="left|center_vertical"
                        android:background="@drawable/oge_kahve" />

                    <Switch
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/sec2_id"
                        android:layout_gravity="right|center_vertical" />

                </FrameLayout>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_gravity="center_horizontal|bottom">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView4"
                        android:layout_gravity="left|center_vertical"
                        android:background="@drawable/oge_ay" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Uyku"
                        android:id="@+id/textView18"
                        android:layout_gravity="center"
                        android:textColor="@color/Black"
                        android:textStyle="bold" />

                    <Switch
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/sec3_id"
                        android:layout_gravity="right|center_vertical" />
                </FrameLayout>

            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:layout_gravity="center">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView5"
                    android:layout_gravity="left|center_vertical"
                    android:background="@drawable/oge_yemek" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Yiyecek"
                    android:id="@+id/textView19"
                    android:layout_gravity="center"
                    android:textColor="@color/Black"
                    android:textStyle="bold" />

                <Switch
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/sec4_id"
                    android:layout_gravity="right|center_vertical" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="106dp"
                android:layout_gravity="center_horizontal|bottom">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="33dp"
                    android:layout_gravity="center_horizontal|top">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Yürüyüş"
                        android:id="@+id/textView20"
                        android:layout_gravity="center"
                        android:textColor="@color/Black"
                        android:textStyle="bold" />

                    <Switch
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/sec5_id"
                        android:layout_gravity="right|center_vertical" />

                    <ImageButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageButton5"
                        android:layout_gravity="left|center_vertical"
                        android:background="@drawable/oge_adim" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_gravity="center">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Koşu"
                        android:id="@+id/textView21"
                        android:layout_gravity="center"
                        android:textStyle="bold"
                        android:textColor="@color/Black" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView6"
                        android:layout_gravity="left|center_vertical"
                        android:background="@drawable/oge_kosu" />

                    <Switch
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/sec6_id"
                        android:layout_gravity="right|center_vertical" />

                </FrameLayout>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_gravity="center_horizontal|bottom">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Bisiklet Sürme"
                        android:id="@+id/textView22"
                        android:layout_gravity="center"
                        android:textColor="@color/Black"
                        android:textStyle="bold" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView7"
                        android:layout_gravity="left|center_vertical"
                        android:background="@drawable/oge_bisiklet" />

                    <Switch
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/sec7_id"
                        android:layout_gravity="right|center_vertical" />
                </FrameLayout>
            </FrameLayout>
        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="249dp"
            android:layout_gravity="center_horizontal|bottom">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="106dp"
                android:layout_gravity="center_horizontal|bottom">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView13"
                    android:layout_gravity="left|top"
                    android:background="@drawable/oge_esneme" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView14"
                    android:layout_gravity="left|center_vertical"
                    android:background="@drawable/oge_bukme" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Esneme"
                    android:id="@+id/textView26"
                    android:layout_gravity="center_horizontal|top"
                    android:textColor="@color/Black"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Bacak Bükme"
                    android:id="@+id/textView27"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:textColor="@color/Black" />

                <Switch
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/sec14_id"
                    android:layout_gravity="right|top" />

                <Switch
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/sec15_id"
                    android:layout_gravity="right|center_vertical" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView16"
                    android:layout_gravity="left|top" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView17"
                    android:layout_gravity="left|center_vertical" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView18"
                    android:layout_gravity="left|bottom" />

            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="118dp"
                android:layout_gravity="center_horizontal|top">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView12"
                    android:layout_gravity="left|center_vertical"
                    android:background="@drawable/oge_doga" />

                <Switch
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/sec12_id"
                    android:layout_gravity="right|center_vertical" />

                <Switch
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/sec13_id"
                    android:layout_gravity="right|bottom" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Doğa Yürüyüşü"
                    android:id="@+id/textView30"
                    android:layout_gravity="center"
                    android:textColor="@color/Black"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Kol Bükme"
                    android:id="@+id/textView31"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textStyle="bold"
                    android:textColor="@color/Black" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView19"
                    android:layout_gravity="left|top" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView20"
                    android:layout_gravity="left|center_vertical" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView21"
                    android:layout_gravity="left|bottom" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView22"
                    android:layout_gravity="left|bottom"
                    android:background="@drawable/oge_kol" />

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal|top">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Bacak Kaldırma"
                        android:id="@+id/textView25"
                        android:layout_gravity="center_horizontal|bottom"
                        android:singleLine="true"
                        android:textColor="@color/Black"
                        android:textStyle="bold" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView10"
                        android:layout_gravity="left|bottom"
                        android:background="@drawable/oge_bacak" />

                    <Switch
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/sec10_id"
                        android:layout_gravity="right|bottom" />

                </FrameLayout>
            </FrameLayout>
        </FrameLayout>

    </FrameLayout>

</RelativeLayout>
