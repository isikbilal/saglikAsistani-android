<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.muslu.saglikasistani.MainActivity"
    tools:showIn="@layout/activity_main">
    <LinearLayout
        android:id="@+id/tab1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:weightSum="1"
        android:baselineAligned="false"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        tools:ignore="RtlHardcoded,UselessParent">


        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="72dp"

            android:layout_weight="0.45">

            <FrameLayout
                android:layout_width="97dp"
                android:layout_height="84dp"
                android:layout_gravity="left|center_vertical">

                <ImageButton
                    android:layout_width="80dp"
                    android:layout_height="70dp"
                    android:id="@+id/kosu_id"
                    android:onClick="Tiklandi"
                    android:layout_gravity="center_horizontal|top"
                    android:background="@drawable/kosu"
                    tools:ignore="ContentDescription" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Koşu"
                    android:id="@+id/textView2"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="@color/Green"
                    android:textStyle="bold"
                    tools:ignore="HardcodedText" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="97dp"
                android:layout_height="84dp"
                android:layout_gravity="center">

                <ImageButton
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:id="@+id/yemek_id"
                    android:onClick="Tiklandi"
                    android:layout_gravity="center_horizontal|top"
                    android:background="@drawable/yemek"
                    tools:ignore="ContentDescription" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Beslenme"
                    android:id="@+id/textView3"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="@color/Green"
                    android:textStyle="bold"
                    tools:ignore="HardcodedText" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="87dp"
                android:layout_height="84dp"
                android:layout_gravity="right|center_vertical"
                android:layout_marginLeft="120dp"
                tools:ignore="RtlHardcoded">

                <ImageButton
                    android:layout_width="match_parent"
                    android:layout_height="51dp"
                    android:id="@+id/veri_id"
                    android:onClick="Tiklandi"
                    android:layout_gravity="center_horizontal|top"
                    android:background="@drawable/uyku"
                    tools:ignore="ContentDescription" />

                <TextView
                    android:layout_width="38dp"
                    android:layout_height="wrap_content"
                    android:text="Uyku"
                    android:id="@+id/textView4"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textColor="@color/Green"
                    android:textStyle="bold"
                    tools:ignore="HardcodedText" />
            </FrameLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Hedefler"
                android:id="@+id/adimTxt_id"
                android:onClick="Tiklandi"
                android:layout_gravity="left|top"
                android:layout_marginTop="10dp"
                android:textColor="@color/Black"
                android:textSize="20dp"
                tools:ignore="HardcodedText,SpUsage" />


        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="73dp"
            android:layout_gravity="left|center_vertical"

            android:id="@+id/adim_id"
            android:onClick="Tiklandi"
            android:layout_weight="0.17">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Günlük toplam Adım"
                android:id="@+id/textView5"
                android:layout_gravity="top|center_horizontal"
                android:textColor="@color/Green"
                android:textStyle="bold"
                tools:ignore="HardcodedText" />

            <ImageView
                android:layout_width="41dp"
                android:layout_height="41dp"
                android:id="@+id/imageView"
                android:layout_gravity="left|center_vertical"
                android:background="@drawable/adim"
                tools:ignore="ContentDescription" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView24"
                android:layout_gravity="left|center_vertical"
                android:background="@drawable/chronomether2"
                android:layout_marginLeft="50dp"
                tools:ignore="ContentDescription,RtlHardcoded" />

            <ImageButton
                android:layout_width="89dp"
                android:layout_height="89dp"
                android:id="@+id/ogeleri_yonet_id"
                android:onClick="Tiklandi"
                android:layout_gravity="right|top"
                android:adjustViewBounds="false"
                android:background="@drawable/arti"
                tools:ignore="ContentDescription,RtlHardcoded" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Öğeleri Yönet"
                android:id="@+id/textView62"
                android:layout_gravity="right|center_vertical"
                android:layout_marginTop="40dp"
                android:textColor="@color/Black"
                tools:ignore="HardcodedText" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="0"
                android:id="@+id/Txt_id"
                android:layout_gravity="center"
                tools:ignore="HardcodedText" />

        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="156dp"
            android:layout_weight="0.49">


            <ImageButton
                android:layout_width="103dp"
                android:layout_height="106dp"
                android:id="@+id/imageButton2"
                android:onClick="Tiklandi"
                android:layout_gravity="center_horizontal|top"
                android:background="@drawable/beslenmeekle"
                android:layout_margin="0dp"
                tools:ignore="ContentDescription" />

            <ImageButton
                android:layout_width="97dp"
                android:layout_height="97dp"
                android:id="@+id/imageButton7"
                android:layout_gravity="right|top"
                android:background="@drawable/uykuekle"
                tools:ignore="ContentDescription" />

            <Button
                android:id="@+id/uykuEkle"
                android:onClick="Tiklandi"
                android:text="uyku_ekle"
                android:textColor="#64DD17"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:background="@drawable/selector_btn_uyku"
                android:shadowColor="#A8A8A8"
                android:shadowDx="0"
                android:shadowDy="6"
                android:shadowRadius="5"
                android:layout_gravity="right|center_vertical"
                tools:ignore="HardcodedText" />



            <FrameLayout
                android:layout_width="98dp"
                android:layout_height="wrap_content"
                >

                <ImageButton
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/imageButton"
                    android:onClick="Tiklandi"
                    android:layout_gravity="left|top"
                    android:background="@drawable/kosuekle"
                    android:contentDescription=""
                    tools:ignore="ContentDescription" />
            </FrameLayout>

            <Button

                android:id="@+id/baslat_id"
                android:text="KOŞU EKLE"
                android:textColor="#64DD17"
                android:onClick="Tiklandi"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:background="@drawable/selector_btn_kosu"
                android:shadowColor="#9C9C9C"
                android:shadowDx="0"
                android:shadowDy="-2"
                android:shadowRadius="0"
                android:layout_gravity="left|center_vertical"
                tools:ignore="HardcodedText" />

            <Button
                android:id="@+id/yemek_ekle_id"
                android:text="Yemek ekle"
                android:textColor="#64DD17"
                android:onClick="Tiklandi"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:background="@drawable/selector_btn_yemek"
                android:shadowColor="#A8A8A8"
                android:shadowDx="0"
                android:shadowDy="6"
                android:shadowRadius="5"
                android:layout_gravity="center"
                tools:ignore="HardcodedText" />

        </FrameLayout>

    </LinearLayout>

</RelativeLayout>
